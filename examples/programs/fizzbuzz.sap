defineFunction fizzbuzz(n)
    if n % 15 == 0 then
        display "fizzbuzz"
    otherwise
        if n % 3 == 0 then
            display "fizz"
        otherwise
            if n % 5 == 0 then
                display "buzz"
            otherwise
                display n
            end
        end
    end
end


set i = 1
repeat 100 times
    fizzbuzz(i)
    set i = i + 1
end